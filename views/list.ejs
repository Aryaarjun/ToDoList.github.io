<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= ListTitle %></h1>
    </div>

    <div class="box">
        <% for(var i = 0; i < setItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p style="text-transform: capitalize;"><%= setItems[i] %></p>
            </div>
            
        <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="Add item" autocomplete="off"> <!--autocomplete off will stop auto suggesting words.-->  
            <button type="submit" name="list" value = <%= ListTitle %>> + </button>
        </form>
    </div>
    <%- include("footer") -%>
  